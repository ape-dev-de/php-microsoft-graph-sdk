{
    "name": "ape-dev-de/microsoft-graph-sdk",
    "description": "Automatically generated sdk by openapi specifications",
    "type": "library",
    "autoload": {
        "psr-4": {
            "ApeDevDe\\MicrosoftGraphSdk\\": [
                "build/",
                "src/"
            ]
        }
    },
    "authors": [
        {
            "name": "Alexis Peters",
            "email": "info@alexispeters.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/property-info": "^7.3",
        "symfony/property-access": "^7.3",
        "phpdocumentor/reflection-docblock": "^5.6",
        "symfony/serializer": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "vlucas/phpdotenv": "^5.5",
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/psr7": "^2.6",
        "symfony/console": "^6.0|^7.0",
        "symfony/yaml": "^6.0|^7.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "ApeDevDe\\MicrosoftGraphSdk\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "analyze": "phpstan analyze src build"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
