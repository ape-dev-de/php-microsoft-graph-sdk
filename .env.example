# Microsoft Graph API Configuration
# 
# EASY SETUP: Run `php auth-setup.php` for interactive configuration!
# This will automatically fill in all values below.
#
# Or manually copy this file to .env and fill in your credentials

# Authentication Method: bearer or client_credentials
GRAPH_AUTH_METHOD=client_credentials

# For Bearer Token Authentication (from auth-setup.php or manual)
GRAPH_BEARER_TOKEN=your_bearer_token_here
GRAPH_REFRESH_TOKEN=your_refresh_token_here

# For Client Credentials (OAuth 2.0)
GRAPH_TENANT_ID=your_tenant_id_here
GRAPH_CLIENT_ID=your_client_id_here
GRAPH_CLIENT_SECRET=your_client_secret_here

# API Configuration
GRAPH_API_VERSION=v1.0
GRAPH_BASE_URL=https://graph.microsoft.com

# Test Configuration
TEST_USER_ID=test_user_id_here
TEST_GROUP_ID=test_group_id_here
TEST_SKIP_DESTRUCTIVE=true
